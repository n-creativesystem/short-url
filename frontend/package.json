{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "rm -rf \"../pkg/infrastructure/router/static/app\" && IS_DEV=true IS_BUILD=true yarn build && mv dist \"../pkg/infrastructure/router/static/app\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:ts:project": "tsc --pretty --noEmit --project ./tsconfig.json",
    "preview": "vite preview",
    "gql:gen": "graphql-codegen --config graphql.config.yaml",
    "test": "NODE_ENV=test vitest run",
    "swag:gen": "openapi-typescript ../pkg/interfaces/router/swagger/ui/docs/swagger.json -o ./src/openapi/schema.d.ts",
    "api:gen": "openapi2aspida -c aspida.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@aspida/axios": "^1.12.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.1",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.0.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.131",
    "@mui/material": "^5.13.2",
    "@mui/system": "^5.14.9",
    "@mui/x-data-grid": "^6.5.0",
    "@react-hook/copy": "^2.0.1",
    "aspida": "^1.14.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "framer-motion": "^10.12.14",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.1",
    "react-router-dom": "^6.11.2",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jsdom": "^22.1.0",
    "openapi-typescript": "^6.2.6",
    "openapi2aspida": "^0.23.0",
    "prettier": "2.8.8",
    "type-fest": "^3.11.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vitest": "^0.34.0"
  }
}
